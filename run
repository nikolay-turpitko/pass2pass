#!/bin/bash

rm -rf ./out

source ./env-$USER

cat sample.csv | \
    go run src/pass2pass/cmd/pass2pass.go \
        -parser lastpass \
        -store plain \
        -path-cleaner  './templates/util/path-clean' \
        -path-filter   './templates/util/path-filter' \
        -path-replacer './templates/util/path-replace' \
        "./templates/multiline-scheme" \
        "./out"

